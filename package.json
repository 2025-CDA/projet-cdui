{
    "name": "easypae",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "serve": "symfony serve -no-tls",
        "dev": "vite",
        "start": "concurrently \"npm run serve\" \"npm run dev\"",
        "mailpit": "mailpit --listen 0.0.0.0:8025 --smtp 0.0.0.0:1025",
        "messenger": "symfony console messenger:consume async -vv",
        "mail": "concurrently \"npm run mailpit\" \"npm run messenger\"",
        "build": "vite build",
        "lint": "eslint .",
        "preview": "vite preview"
    },
    "dependencies": {
        "@emotion/react": "^11.14.0",
        "@emotion/styled": "^11.14.1",
        "@mui/material": "^7.3.4",
        "axios": "^1.12.2",
        "lucide-react": "^0.545.0",
        "preline": "^3.2.3",
        "react": "^19.1.1",
        "react-dom": "^19.1.1",
        "react-router-dom": "^7.9.2",
        "styled-components": "^6.1.19",
        "tailwind-styled-components": "^2.2.0",
        "uuid": "^13.0.0"
    },
    "devDependencies": {
        "@eslint/js": "^9.35.0",
        "@tailwindcss/postcss": "^4.1.13",
        "@tailwindcss/vite": "^4.1.13",
        "@types/react": "^19.1.13",
        "@types/react-dom": "^19.1.9",
        "@vitejs/plugin-react": "^5.0.2",
        "autoprefixer": "^10.4.21",
        "concurrently": "^9.2.1",
        "eslint": "^9.35.0",
        "eslint-plugin-react-hooks": "^5.2.0",
        "eslint-plugin-react-refresh": "^0.4.20",
        "globals": "^16.4.0",
        "postcss": "^8.5.6",
        "tailwindcss": "^4.1.13",
        "vite": "^7.1.6"
    }
}
